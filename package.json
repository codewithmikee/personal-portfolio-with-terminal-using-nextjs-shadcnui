{
  "name": "ultimate-terminal-supporting-portfolio-builder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:admin": "turbo dev --filter=portfolio-builder-admin",
    "build:admin": "turbo build --filter=portfolio-builder-admin",
    "dev:web": "turbo dev --filter=portfolio-web",
    "build:web": "turbo build --filter=portfolio-web",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.6.2",
    "tsc": "^2.0.4",
    "turbo": "^2.5.5",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
